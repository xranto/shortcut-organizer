{
  "manifest_version": 3,
  "name": "Shortcut Organizer",
  "description": "Extension Chrome offrant un tableau de bord élégant et personnalisable pour organiser vos raccourcis web.",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "favicon.png"
  },
  "incognito": "split",
  "permissions": [
    "storage",
    "favicon",
	"tabs"
  ],
  "icons": {
   "128": "extension_icon128.png"
  }
}