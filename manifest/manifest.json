{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "description": "__MSG_appDescription__",
  "default_locale": "fr",
  "version": "1.0.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "favicon.png"
  },
  "incognito": "split",
  "permissions": [
    "storage",
    "favicon",
	"tabs"
  ],
  "icons": {
   "128": "extension_icon128.png"
  }
}